{
  "name": "esbuild-plugin-react-virtualized",
  "description": "ESBuild plugin for fixing react-virtualized",
  "version": "1.0.6",
  "license": "MIT",
  "author": "Adam Bouqdib <adam@abemedia.co.uk>",
  "type": "module",
  "packageManager": "pnpm@10.30.3",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "prepare": "husky",
    "dev": "tsdown --watch",
    "build": "tsdown",
    "lint": "biome check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abemedia/esbuild-plugin-react-virtualized.git"
  },
  "bugs": {
    "url": "https://github.com/abemedia/esbuild-plugin-react-virtualized/issues"
  },
  "homepage": "https://github.com/abemedia/esbuild-plugin-react-virtualized#readme",
  "peerDependencies": {
    "esbuild": "*"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/node": "^24.11.0",
    "esbuild": "^0.27.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.3.0",
    "prettier": "^3.8.1",
    "tsdown": "0.20.3",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*": "biome check --write --no-errors-on-unmatched --files-ignore-unknown=true",
    "*.{md,yml}": "prettier --write"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "esbuild"
    ]
  }
}
